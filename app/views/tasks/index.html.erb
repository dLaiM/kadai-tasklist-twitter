<h1>タスク一覧</h1>

<ul>
    <% @tasks.each do |task|%>
      <li><%= link_to task.id, task %> : <%= task.status %> &gt; <%= task.content %></li>
    <% end%>
</ul>

<%= link_to "新規タスクの保存", new_task_path %>
<div class="center jumbotron">
  <div class="text-center">
    <h1>ユーザ登録をしてください！</h1>
    <%= link_to 'Sign up now!', signup_path, class: 'btn btn-lg btn-primary' %>
  </div>
</div>